# PCO API Wrapper - Environment Configuration
# Copy this file to .env and fill in your actual values

# Planning Center Online API Credentials
# Get these from: https://planning.center/settings/developer
PCO_APP_ID=your_application_id_here
PCO_SECRET=your_application_secret_here

# Flask Configuration
FLASK_DEBUG=False
FLASK_HOST=0.0.0.0
FLASK_PORT=5000

# Optional: Logging Level
LOG_LEVEL=INFO